
<div class="my-scribble-page">
    <div class="my-scribble-h1">
    <h1>My Scribbles</h1>
</div>

    <div class="scribbles-category-container">
  {{#each scribbles}}
  <div class="scribbles-category-linker">
    <a href="/scribbles/{{this._id}}" class="scribbles-category-linker">
      <div class="scribbles-category-card">
        <div class="scribbles-category-card-image">
          <img src="{{ImageUrl}}" class="scribbles-category-card-img" alt="{{title}}">
        </div>
        <div class="scribbles-category-card-content">
          <p class="scribbles-category-card-title">{{title}}</p>
          <p class="scribbles-category-card-description">{{description}}</p>
           <div class="scribbles-category-likes-container">
            <i class="fa fa-heart heart"></i>
            <span class="scribbles-category-likes-count">{{this.likes.length}}</span>
          </div>
        </div>
      </div>
    </a>
     <div class="my-Scribbles-links">
                <div class="editting-btn"><a href="/scribbles/edit/{{this._id}}" onclick="return confirm('Are you sure you want to edit this scribble?');" class="btn btn-danger">Edit Scribble</a></div>
                <div class="editting-btn"><a href="/scribbles/delete/{{scribble._id}}" onclick="return confirm('Are you sure you want to delete this scribble?');" class="btn btn-danger delete-button-owner myscribble-link-btn">Delete</a></div>
            </div>
  </div>
  
  {{else}}
  <p>There are no posts yet!</p>
  {{/each}}
</div>
</div>